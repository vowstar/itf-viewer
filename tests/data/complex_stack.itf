TECHNOLOGY = generic_complex
GLOBAL_TEMPERATURE = 85.0
REFERENCE_DIRECTION = VERTICAL
BACKGROUND_ER = 1.0

$ Substrate and diffusion layers
DIELECTRIC substrate {THICKNESS=2.0 ER=11.9}
CONDUCTOR nwell {CRT1=5.0E-03 CRT2=0.0 THICKNESS=0.5 RPSQ=500.0}
CONDUCTOR pdiff {CRT1=8.0E-03 CRT2=0.0 THICKNESS=0.1 RPSQ=100.0}
CONDUCTOR ndiff {CRT1=8.0E-03 CRT2=0.0 THICKNESS=0.1 RPSQ=80.0}

$ Gate stack
DIELECTRIC gate_oxide {THICKNESS=0.005 ER=3.9}
CONDUCTOR gate {CRT1=2.0E-03 CRT2=0.0 THICKNESS=0.15 WMIN=0.05 SMIN=0.05 RPSQ=20.0}

$ Spacer and salicide block
DIELECTRIC spacer {THICKNESS=0.08 ER=4.0}
DIELECTRIC sab {THICKNESS=0.02 ER=7.0}

$ Contact level
DIELECTRIC pmd {THICKNESS=0.6 ER=4.2 SW_T=0.3 TW_T=0.4}
CONDUCTOR contact {CRT1=1.0E-03 CRT2=0.0 THICKNESS=0.4 RPSQ=2.0}

$ First metal level
DIELECTRIC imd1 {THICKNESS=0.8 ER=4.2}
CONDUCTOR m1 {CRT1=2.5E-03 CRT2=-4.38E-07 THICKNESS=0.35 WMIN=0.07 SMIN=0.07 RPSQ=0.025
    RHO_VS_WIDTH_AND_SPACING {
        WIDTHS   {0.07 0.14 0.21 0.35}
        SPACINGS {0.07 0.105 0.14 0.21}
        VALUES   {0.040 0.035 0.028 0.025
                  0.032 0.030 0.026 0.024
                  0.028 0.026 0.024 0.023
                  0.022 0.022 0.021 0.020}
    }
}

$ Second metal level
DIELECTRIC imd2 {THICKNESS=1.0 ER=4.2}
CONDUCTOR m2 {CRT1=2.8E-03 CRT2=0.0 THICKNESS=0.4 WMIN=0.09 SMIN=0.09 RPSQ=0.018}

$ Third metal level
DIELECTRIC imd3 {THICKNESS=1.2 ER=4.2}
CONDUCTOR m3 {CRT1=3.2E-03 CRT2=1.0E-07 THICKNESS=0.5 WMIN=0.12 SMIN=0.12 RPSQ=0.012}

$ Top level thick metal
DIELECTRIC imd4 {THICKNESS=2.0 ER=4.2}
CONDUCTOR top_metal {CRT1=3.8E-03 CRT2=7.0E-08 THICKNESS=2.0 WMIN=1.0 SMIN=1.0 RPSQ=0.008}

$ Passivation
DIELECTRIC pass1 {THICKNESS=0.5 ER=7.0}
DIELECTRIC pass2 {THICKNESS=1.0 ER=4.2}

$ Via definitions
VIA cont_pdiff {FROM=pdiff TO=contact AREA=0.005 RPV=80.0}
VIA cont_ndiff {FROM=ndiff TO=contact AREA=0.005 RPV=70.0}
VIA cont_gate {FROM=gate TO=contact AREA=0.004 RPV=100.0}
VIA via_c_m1 {FROM=contact TO=m1 AREA=0.008 RPV=25.0}
VIA via_m1_m2 {FROM=m1 TO=m2 AREA=0.012 RPV=8.0}
VIA via_m2_m3 {FROM=m2 TO=m3 AREA=0.016 RPV=6.0}
VIA via_m3_top {FROM=m3 TO=top_metal AREA=0.25 RPV=1.5}